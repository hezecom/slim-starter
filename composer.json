{
    "name": "hezecom/slimstarter",
    "description": "Slim Framework 4 Starter App",
    "type": "project",
    "keywords": ["slim", "slim 4", "skeleton", "authentication","template", "orm","hezecom"],
    "homepage": "https://github.com/hezecom/slimapp",
    "license": "MIT",
    "authors": [
        {
            "name": "Hezekiah Omotsuebe",
            "email": "support@hezecom.com"
        }
    ],
    "require": {
        "php": "^7.4 | 8.*",
        "slim/slim": "4.*",
        "slim/twig-view": "^3.2",
        "monolog/monolog": "^2.3",
        "illuminate/database": "^8.60",
        "respect/validation": "^2.0",
        "slim/csrf": "^1.2",
        "slim/flash": "^0.4",
        "vlucas/phpdotenv": "^5.3",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.5",
        "delight-im/auth": "^8.3",
        "phpmailer/phpmailer": "^6.5",
        "filp/whoops": "^2.14",
        "ext-pdo": "*"
    },
    "autoload": {
    	"psr-4": {
    		"App\\": "app/"
    	}
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    }
}
